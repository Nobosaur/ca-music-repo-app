<%= form_for(@userEdit) do |f| %>  
    <div class="field"> 
      Name:<br><%= f.text_area :first_name %><br>
       Last name:<br><%= f.text_area :last_name %><br>
        Email:<br><%= f.text_area :email %><br>
        Role:<br><%= f.text_area :role %><br>
    </div> 
    <div class="actions"> 
      <%= f.submit "Save Changes" %> 
    </div> 
<% end %>